{
  "name": "Contractor",
  "module": "FLRTS Extensions",
  "doctype": "DocType",
  "engine": "InnoDB",
  "is_submittable": 0,
  "autoname": "field:contractor_name",
  "fields": [
    {
      "fieldname": "contractor_name",
      "fieldtype": "Data",
      "label": "Contractor Name",
      "reqd": 1,
      "unique": 1,
      "in_list_view": 1,
      "description": "Full legal name of the contractor or company"
    },
    {
      "fieldname": "contractor_type",
      "fieldtype": "Select",
      "label": "Contractor Type",
      "options": "Equipment Maintenance\nElectrical\nPlumbing\nHVAC\nGeneral Labor\nOther",
      "reqd": 0,
      "in_list_view": 1,
      "description": "Primary service type provided by contractor"
    },
    {
      "fieldname": "contact_email",
      "fieldtype": "Data",
      "label": "Contact Email",
      "options": "Email",
      "reqd": 0,
      "in_list_view": 1,
      "description": "Primary contact email address"
    },
    {
      "fieldname": "contact_phone",
      "fieldtype": "Data",
      "label": "Contact Phone",
      "options": "Phone",
      "reqd": 0,
      "in_list_view": 1,
      "description": "Primary contact phone number"
    },
    {
      "fieldname": "is_active",
      "fieldtype": "Check",
      "label": "Active",
      "default": "1",
      "in_list_view": 1,
      "description": "Whether the contractor is currently available for work"
    }
  ],
  "permissions": [
    {
      "role": "System Manager",
      "read": 1,
      "write": 1,
      "create": 1,
      "delete": 1
    },
    {
      "role": "Sales Manager",
      "read": 1,
      "write": 1,
      "create": 1,
      "delete": 0
    },
    {
      "role": "Sales User",
      "read": 1,
      "write": 0,
      "create": 0,
      "delete": 0
    }
  ],
  "title_field": "contractor_name"
}
